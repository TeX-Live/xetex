\par {\listtext\pard\plain\s1 \b\f1\fs32 \hich\af1\dbch\af0\loch\f1 1\tab}}\pard\plain \s1\ql \fi-432\li432\ri0\sb600\keepn\widctlpar\jclisttab\tx432\aspalpha\aspnum\faauto\ls102\outlinelevel0\adjustright\rin0\lin432\itap0 
