all:
	(cd xpdf; $(MAKE) libxpdf.a)
	(cd goo; $(MAKE) libGoo.a)
	(cd fofi; $(MAKE) libfofi.a)

mostlyclean clean depend:
	(cd xpdf; $(MAKE) $@)
	(cd goo; $(MAKE) $@)
	(cd fofi; $(MAKE) $@)

distclean maintainer-clean extraclean: clean
	(cd xpdf; $(MAKE) $@)
	(cd goo; $(MAKE) $@)
	(cd fofi; $(MAKE) $@)
	rm -f Makefile config.cache config.log config.status aconf.h
	rm -f fofi/Makefile.aux goo/Makefile.aux xpdf/Makefile.aux kpse_empty
