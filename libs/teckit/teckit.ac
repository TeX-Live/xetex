if test "$needs_teckit" = no; then
  TECKITCPPFLAGS=
  LDTECKIT=
  TECKITDEP=
else
  TECKITCPPFLAGS='-I$(TECKITSRCDIR)/source/Public-headers'
  LDTECKIT='$(TECKITDIR)/lib/.libs/libTECkit.a'
  TECKITDEP='$(TECKITDIR)/lib/.libs/libTECkit.a'
fi

AC_SUBST(TECKITCPPFLAGS)
AC_SUBST(LDTECKIT)
AC_SUBST(TECKITDEP)
