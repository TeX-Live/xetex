# FreeType 2 src/psaux Jamfile (c) 2001 David Turner
#

SubDir  FT2_TOP src psaux ;

SubDirHdrs  [ FT2_SubDir  src psaux ] ;

{
  local  _sources ;

  if $(FT2_MULTI)
  {
    _sources = psauxmod psobjs t1decode ;
  }
  else
  {
    _sources = psaux ;
  }

  Library  $(FT2_LIB) : $(_sources).c ;
}

# end of src/psaux Jamfile
