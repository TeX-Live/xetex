s%@LIBXPDFCPPFLAGS@%$LIBXPDFCPPFLAGS%g
s%@LDLIBXPDF@%$LDLIBXPDF%g
s%@LIBXPDFDEP@%$LIBXPDFDEP%g
s%@LIBT1CPPFLAGS@%$LIBT1CPPFLAGS%g
s%@LDLIBT1@%$LDLIBT1%g
s%@LIBT1DEP@%$LIBT1DEP%g
