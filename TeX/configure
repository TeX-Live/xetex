  withval="$with_x_toolkit"
  	  case "${withval}" in
	    y | ye | yes )	val=yes ;;
	    n | no )		val=no  ;;
	    neXtaw )		val=neXtaw  ;;
	    xaw3d )		val=xaw3d  ;;
	    x | xa | xaw )	val=xaw ;;
	    a | at | ath | athe | athena ) val=athena ;;
	    m | mo | mot | moti | motif ) val=motif ;;
	    * )
echo "configure: warning: unknown --with-x-toolkit argument \`$withval'; using yes." 1>&2
              val=yes;;
	  esac
	  with_x_toolkit=$val

fi



