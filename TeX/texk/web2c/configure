  # Create build dir, if necessary.
  test -d etexdir || mkdir etexdir
  test "${with_etex+set}" = set || with_etex=yes
else
  if test "x$with_etex" = xyes; then
    echo "configure: warning: The e-TeX sources are not available." 1>&2
  fi
  with_etex=no
fi
