done

    if test "$ac_cv_header_zlib_h" != yes; then
      echo "configure: warning: zlib.h not found. Using my own zlib sources." 1>&2
      with_system_zlib=no
    fi
  fi

  LDFLAGS=$OLD_LDFLAGS
