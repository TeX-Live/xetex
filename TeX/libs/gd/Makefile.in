ac_include ../../texk/make/common.mk
ac_include ../../texk/make/programs.mk
ac_include ../../texk/make/library.mk

ZLIBDIR=../zlib
ZLIBSRCDIR=$(srcdir)/$(ZLIBDIR)

LIBPNGDIR=../libpng
LIBPNGSRCDIR=$(srcdir)/$(LIBPNGDIR)

XDEFS = -DHAVE_LIBPNG @ZLIBCPPFLAGS@ @LIBPNGCPPFLAGS@

OBJS = gd.o gdfx.o gd_gd.o gd_gd2.o gd_io.o gd_io_dp.o gd_io_file.o gd_io_ss.o gd_jpeg.o gd_png.o gd_ss.o gd_topal.o gd_wbmp.o gdcache.o gdfontg.o gdfontl.o gdfontmb.o gdfonts.o gdfontt.o gdft.o gdhelpers.o gdkanji.o gdtables.o gdxpm.o wbmp.o
all: libgd.a

libgd.a: $(OBJS)
	rm -f $@
	$(AR) $(ARFLAGS) $@ $(OBJS)
	$(RANLIB) $@

mostlyclean clean:
	rm -f *.o libgd.a

distclean realclean extraclean maintainer-clean: clean
	rm -f config.status config.log Makefile config.h config.cache
